			<Label class="CharacterNameLabel Headline1" id="HeroName" text="DRAGON KNIGHT" />
		</Panel>
		<Panel hittest="false" class="HeroFrame Bot">
			<Panel hittest="false" class="HeroFrame Bot Left" id="BotLeft">
				<Label class="CharacterInfo CharacterBioLabel Headline1" id="HeroBioLabel" text="BIOGRAPHY" />
				<Label hittest="false" class="CharacterInfo Attribute Str Headline1" id="str" text="10" />
				<Label hittest="false" class="CharacterInfo Attribute Agi Headline1" id="agi" text="10" />
				<Label hittest="false" class="CharacterInfo Attribute Int Headline1" id="int" text="10" />
				<Label class="CharacterInfo CharacterDescrLabel SimpleText" id="HeroBio" text="AN HERO" />
			</Panel>
			<Panel hittest="false" class="HeroFrame Bot Right" id="BotRight">
				<Label class="CharacterInfo CharacterAbilitiesLabel Headline1" id="AbilitiesLabel" text="ABILITIES" />
				<Panel hittest="true" class="Abilities" id="AbilitiesPanel">
				</Panel>
			</Panel>
		</Panel>
	</Panel>
